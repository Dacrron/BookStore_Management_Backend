@model IEnumerable<Bookietry.Models.BooksSeller>

@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table table-dark table-borderless table-hover">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.Seller_Name)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Seller_Number)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Seller_email)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Seller_address)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Seller_Name)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Seller_Number)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Seller_email)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Seller_address)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.Seller_id">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.Seller_id">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.Seller_id">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
